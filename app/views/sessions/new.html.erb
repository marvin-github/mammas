
<div class="container">
  <h1>Login</h1>
<%= form_tag '/login' do %>
    <div class="form-group">
      <%= label_tag :email, "User Name:" %>
      <%= text_field_tag :email, nil, class: "form-control" %>
    </div>
    <div class="form-group">
      <%= label_tag :password, "Password:" %>
      <%= text_field_tag :password, nil, class: "form-control" %>
    </div>
      <div class="form-group">
        <%= submit_tag "Submit" %>
      </div>


<% end %>
</div>
